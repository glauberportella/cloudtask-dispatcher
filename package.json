        "jest": "^29.6.2",
        "ts-jest": "^29.1.1",
        "typescript": "^5.1.6"
    },
    "publishConfig": {
        "access": "public"
    },
    "dependencies": {
        "@google-cloud/tasks": "^6.0.1"
    }
