    "version": "1.0.0",
    "description": "Cloud task dispatcher library",
    "type": "module",
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "types": "dist/types/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
